                                <!--Title-->
                                <tr id="id" class="newInfo">
                                    <td class="voteButton"><i id="#" class="fas fa-heart"></i></td>
                                    <td class="title">
                                        <a class="newTitle titleText" href="#"> Item title </a>
                                        <a class="newLink" href="#">(itemlink.com)</a>
                                    </td>
                                </tr>
                                <!--Actions-->
                                <tr class="newActions">
                                    <td colspan="1"></td>
                                    <td>
                                        30 points
                                        &nbsp
                                        by
                                        <a href="#">pepe</a>
                                        &nbsp
                                        <a href="#">20 min ago</a>
                                        |
                                        <a href="#">past</a>
                                        |
                                        <a href="#">hide</a>
                                        |
                                        <a href="#">favorite</a>
                                        |
                                        <a href="#">20 comments</a>
